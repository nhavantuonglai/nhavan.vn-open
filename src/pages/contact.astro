---
import Layout from '~/layouts/page.astro';

import Contact from '~/components/content/main/contact.astro';

import Form from '~/components/content/main/form.astro';

const metadata = {
	title: 'Đăng ký thành viên',
	description: "Diễn đàn luôn rộng cửa chào đón những ai yêu thích, thực hành viết lách mỗi ngày để trở thành nhà văn tương lai. Hãy điều thông tin để trở thành hội viên chính thức, dễ dàng gửi và đăng bài lên diễn đàn ngay hôm nay.",
};
---
<Layout metadata={metadata}>

	<Contact/>

	<Form/>

</Layout>